{
    "manifest_version": 2,
  
    "name": "Discord Token Login",
    "description": "Changes Discord Email Form in to Token Form",
    "version": "0.6",
  
    "icons": {
      "48": "icons/icon-48.png",
      "64": "icons/icon.png"
    },
  
    "permissions": [
      "tabs", "https://discord.com/login"
    ],
  
    "browser_action": {
      "default_icon": "icons/icon.png",
      "default_popup": "popup.html"
    }
  }